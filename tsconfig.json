{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "lib": ["DOM", "DOM.Iterable", "ESNext"],
      "jsx": "react-jsx",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "allowJs": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "noFallthroughCasesInSwitch": true
    },
    "include": ["src"],
    "exclude": ["dist", "node_modules"]
  }
  